{
  "manifest_version": 3,
  "name": "Roblox Game Analytics Tracker",
  "version": "1.0",
  "description": "Track Roblox game analytics like players, revenue estimates, and DevEx status.",
  "permissions": [
    "scripting",
    "activeTab",
    "storage",
    "downloads"
  ],
  "host_permissions": [
    "https://www.roblox.com/games/*",
    "https://games.roblox.com/*",
    "https://economy.roblox.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://www.roblox.com/games/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
